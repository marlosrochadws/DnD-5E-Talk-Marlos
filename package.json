{
  "name": "refactory",
  "version": "1.0.0",
  "description": "A basic static site generator using eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "ELEVENTY_ENV=production npx eleventy",
    "del:assets": "rimraf ./src/compiled-assets",
    "del:dist": "rimraf ./dist",
    "dev": "npm run dev:assets & npm run dev:site",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "dev:site": "ELEVENTY_ENV=development npx eleventy --serve",
    "prod": "npm-run-all del:dist del:assets build:assets build:site",
    "serve:prod": "serve ./dist/",
    "start": "npm run dev:assets & npm run dev:site"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gdo-iprospect-usa/refactory"
  },
  "author": "Chris Cooper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdo-iprospect-usa/refactory/issues"
  },
  "homepage": "https://github.com/gdo-iprospect-usa/refactory#readme",
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@babel/core": "7.17.2",
    "@babel/preset-env": "7.16.11",
    "autoprefixer": "10.4.2",
    "babel-loader": "^8.2.3",
    "core-js": "3.23.3",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "html-minifier": "4.0.0",
    "install": "0.13.0",
    "md5-file": "5.0.0",
    "mini-css-extract-plugin": "2.5.3",
    "npm": "8.13.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "6.2.1",
    "rimraf": "3.0.2",
    "sass": "1.49.7",
    "sass-loader": "12.4.0",
    "serve": "13.0.2",
    "swiper": "^8.4.5",
    "tailwindcss": "^3.0.19",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
